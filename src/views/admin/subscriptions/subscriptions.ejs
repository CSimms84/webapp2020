<!DOCTYPE html>
<html>

<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>Ellipsis | Subscriptions</title>

    <link href="/assets/css/bootstrap.min.css" rel="stylesheet">
    <link href="/assets/font-awesome/css/font-awesome.css" rel="stylesheet">

    <!-- Toastr style -->
    <link href="/assets/css/plugins/toastr/toastr.min.css" rel="stylesheet">
    <link href="/assets/css/plugins/dataTables/datatables.min.css" rel="stylesheet">

    <link href="/assets/css/plugins/select2/select2.min.css" rel="stylesheet">
    <link href="/assets/css/plugins/select2/select2-bootstrap4.min.css" rel="stylesheet">

    <link href="/assets/css/plugins/switchery/switchery.css" rel="stylesheet">

    <link href="/assets/css/plugins/iCheck/custom.css" rel="stylesheet">
    <link href="/assets/css/plugins/jasny/jasny-bootstrap.min.css" rel="stylesheet">
    <link href="/assets/css/plugins/datapicker/datepicker3.css" rel="stylesheet">


    <link href="/assets/css/animate.css" rel="stylesheet">
    <link href="/assets/css/style.css" rel="stylesheet">
    <link href="/assets/css/admin/subscriptions.css" rel="stylesheet">
    <!-- Sweet Alert -->
    <link href="/assets/css/plugins/sweetalert/sweetalert.css" rel="stylesheet">



</head>

<body class="pace-done skin-1">

    <div id="wrapper">
        <%- include('../navbar',{page:'subscriptions'}); %>
            <div id="page-wrapper" class="gray-bg">
                <%- include('../headerbar'); %>
                    <%- include('../breadcrumb',{page:'Subscriptions', items:['Admin', 'Subscriptions','Subscriptions']}); %>

                        <div class="wrapper wrapper-content  animated fadeInRight">
                            <div class="row">
                                <div class="col-sm-8">
                                    <div class="ibox">
                                        <div class="ibox-content">
                                            <span class="text-muted small float-right"><a id="" href="?action=addSubscription"
                                        class="btn btn-primary btn-circle client-link btn-add-subscription"> <i
                                            class="fa fa-plus"></i></a></span>
                                            <h2>Subscriptions</h2>



                                            <div class="clients-list">
                                                <div class="full-height-scroll">
                                                    <div class="table-responsive">
                                                        <table class="table table-striped table-bordered table-hover dataTables-example">
                                                            <thead>
                                                                <tr>
                                                                    <th>Name</th>
                                                                    <th>Role</th>
                                                                    <th>Price</th>
                                                                    <th>Recursion</th>
                                                                    <th>Public</th>
                                                                    <th>Promo</th>
                                                                    <th>Promo price</th>
                                                                    <th>Starts</th>
                                                                    <th>Ends</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <% if (data && data.subscriptions && data.subscriptions.length) { %>
                                                                    <% for(var i=0; i < data.subscriptions.length; i++) {%>
                                                                        <tr>

                                                                            <td>
                                                                                <a href="?action=showSubscription&subscriptionId=<%= data.subscriptions[i]._id %>" class="client-link">
                                                                                    <%= data.subscriptions[i].name %>
                                                                                </a>
                                                                            </td>
                                                                            <td>
                                                                                <a href="?action=showSubscription&subscriptionId=<%= data.subscriptions[i]._id %>" class="client-link">
                                                                                    <%= data.subscriptions[i].role %>
                                                                                </a>
                                                                            </td>
                                                                            <td>
                                                                                <a href="?action=showSubscription&subscriptionId=<%= data.subscriptions[i]._id %>" class="client-link">
                                                                                    <%= data.subscriptions[i].price %>$
                                                                                </a>
                                                                            </td>
                                                                            <td>
                                                                                <a href="?action=showSubscription&subscriptionId=<%= data.subscriptions[i]._id %>" class="client-link">

                                                                                    <% if (data.subscriptions[i].recursion == 'ot') { %>
                                                                                        One time
                                                                                        <% } %>

                                                                                            <% if (data.subscriptions[i].recursion == 'weekly') { %>
                                                                                                Weekly
                                                                                                <% } %>

                                                                                                    <% if (data.subscriptions[i].recursion == 'monthly') { %>
                                                                                                        Monthly
                                                                                                        <% } %>

                                                                                                            <% if (data.subscriptions[i].recursion == 'yearly') { %>
                                                                                                                Yearly
                                                                                                                <% } %>
                                                                                </a>
                                                                            </td>

                                                                            <td class="client-status">
                                                                                <% if (!data.subscriptions[i].public) { %>
                                                                                    <span class="label label-danger">False</span>
                                                                                    <% } else {%>
                                                                                        <span class="label label-primary">True</span>
                                                                                        <% } %>
                                                                            </td>


                                                                            <td class="client-status">
                                                                                <% if (!data.subscriptions[i].promo) { %>
                                                                                    <span class="label label-danger">False</span>
                                                                                    <% } else {%>
                                                                                        <span class="label label-primary">True</span>
                                                                                        <% } %>
                                                                            </td>

                                                                            <td class="client-status">
                                                                                <% if (data.subscriptions[i].promo) { %>

                                                                                    <%= data.subscriptions[i].pricePromo %>$
                                                                                        <% } %>
                                                                            </td>

                                                                            <td class="client-status">
                                                                                <% if (data.subscriptions[i].promo) { %>

                                                                                    <%= data.subscriptions[i].sstarts %>
                                                                                        <% } %>
                                                                            </td>

                                                                            <td class="client-status">
                                                                                <% if (data.subscriptions[i].promo) { %>

                                                                                    <%= data.subscriptions[i].sends %>
                                                                                        <% } %>
                                                                            </td>


                                                                        </tr>
                                                                        <% } %>
                                                                            <% } %>

                                                            </tbody>

                                                        </table>
                                                    </div>
                                                </div>

                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-4">
                                    <div class="ibox selected">

                                        <div class="ibox-content">
                                            <div class="tab-content">
                                                <% if (data && data.action && data.action == 'addSubscription') { %>
                                                    <div id="addSubscription" class="tab-pane active">
                                                        <form role="form" id="form">
                                                            <div class="row m-b-lg">

                                                                <div class="col-sm-12 text-center">
                                                                    <h2>Add subscription</h2>
                                                                </div>

                                                            </div>
                                                            <div class="client-detail">
                                                                <div class="">

                                                                    <div id="error">

                                                                    </div>

                                                                    <div class="form-group  row">
                                                                        <div class="col-sm-12"><label class="col-sm-12 row col-form-label">Name</label><input name="name" id="name" type="text" class="form-control" placeholder="Name" required></div>
                                                                    </div>

                                                                    <div class="form-group  row">
                                                                        <div class="col-sm-12">
                                                                            <label class="col-sm-12 row col-form-label">Role</label>
                                                                            <select id="role" class="form-control" name="role" required>
                                                                <option value="" selected disabled>Select role</option>
                                                                
                                                                <option value="subscriber">Subscriber</option>
                                                                <option value="coach">Coach</option>
                                                                <option value="company">Company</option>
                                                            </select>
                                                                        </div>
                                                                    </div>

                                                                    <div class="form-group  row">
                                                                        <div class="col-sm-12"><label class="col-sm-12 row col-form-label">Price</label><input type="number" id="price" name="price" max="9999" class="form-control" min="1" placeholder="Price"></div>
                                                                    </div>

                                                                    <div class="form-group  row">
                                                                        <div class="col-sm-12">
                                                                            <label class="col-sm-12 row col-form-label">Recursion</label>
                                                                            <select id="recursion" class="form-control" name="recursion" required>
                                                                <option value="" selected disabled>Select recursion</option>
                                                                <option value="ot">One time</option>
                                                                <option value="weekly">Weekly</option>
                                                                <option value="monthly">Monthly</option>
                                                                <option value="yearly">Yearly</option>
                                                            </select>
                                                                        </div>
                                                                    </div>

                                                                    <div class="form-group  row">
                                                                        <div class="col-sm-12">
                                                                            <div class="i-checks"><label> <input id="public" type="checkbox"> <i></i> Publish ? </label></div>
                                                                        </div>
                                                                    </div>

                                                                    <div class="form-group  row">
                                                                        <div class="col-sm-12"><label class="col-sm-12 row col-form-label">Description</label><textarea class="form-control" name="description" id="description" placeholder="Description"></textarea>
                                                                        </div>
                                                                    </div>

                                                                    <div class="form-group  row">
                                                                        <div class="col-sm-12">
                                                                            <div class="i-checks"><label> <input id="promo" type="checkbox"> <i></i> Promo ? </label></div>
                                                                        </div>
                                                                    </div>

                                                                    <div class="form-group  row">
                                                                        <div class="col-sm-12"><label class="col-sm-12 row col-form-label">Promo price</label><input type="number" id="pricePromo" name="pricePromo" max="9999" class="form-control" min="1" placeholder="Promo price"
                                                                                readonly></div>
                                                                    </div>

                                                                    <div class="form-group">
                                                                        <label class="font-normal">Starts</label>
                                                                        <div class="input-group date">
                                                                            <span class="input-group-addon"><i class="fa fa-calendar"></i></span><input id="starts" name="starts" data-mask="99/99/9999" type="text" class="form-control" readonly>
                                                                        </div>
                                                                    </div>

                                                                    <div class="form-group">
                                                                        <label class="font-normal">Ends</label>
                                                                        <div class="input-group date">
                                                                            <span class="input-group-addon"><i class="fa fa-calendar"></i></span><input id="ends" name="ends" data-mask="99/99/9999" type="text" class="form-control" readonly>
                                                                        </div>
                                                                    </div>




                                                                    <div class="form-group  row">
                                                                        <div class="col-md-6 col-sm-12"><button type="submit" id="saveSubscription" class="btn btn-block btn-primary">Save</button></div>
                                                                        <div class="col-md-6 col-sm-12"><a href="/admin/subscriptions" id="cancelSubscription" class="btn btn-block btn-outline btn-danger">Cancel</a>
                                                                        </div>
                                                                        </>
                                                                    </div>
                                                                </div>
                                                        </form>
                                                        </div>
                                                        <% } %>

                                                            <% if (data && data.action && data.action == 'showSubscription' && data.subscriptionId && data.subscription) { %>
                                                                <div id="showUser" class="tab-pane active">
                                                                    <div id="error">

                                                                    </div>
                                                                    <div class="row m-b-lg">
                                                                        <div class="col-lg-4 text-center">
                                                                            <h2>
                                                                                <%= data.subscription.name %>
                                                                            </h2>
                                                                        </div>

                                                                    </div>
                                                                    <div class="client-detail">
                                                                        <div class="">

                                                                            <ul class="list-group clear-list">
                                                                                <li class="list-group-item fist-item">
                                                                                    <strong>Role:</strong>
                                                                                    <%= data.subscription.role %>
                                                                                </li>
                                                                                <li class="list-group-item">
                                                                                    <strong>Price:</strong>

                                                                                    <%= data.subscription.price %> $
                                                                                </li>
                                                                                <li class="list-group-item">
                                                                                    <strong>Recursion:</strong>
                                                                                    <% if (data.subscription.recursion == 'ot') { %>
                                                                                        One time
                                                                                        <% } %>

                                                                                            <% if (data.subscription.recursion == 'weekly') { %>
                                                                                                Weekly
                                                                                                <% } %>

                                                                                                    <% if (data.subscription.recursion == 'monthly') { %>
                                                                                                        Monthly
                                                                                                        <% } %>

                                                                                                            <% if (data.subscription.recursion == 'yearly') { %>
                                                                                                                Yearly
                                                                                                                <% } %>
                                                                                </li>

                                                                                <li class="list-group-item">
                                                                                    <strong>Public:</strong>
                                                                                    <% if (!data.subscription.public) { %>
                                                                                        <span class="label label-danger">False</span>
                                                                                        <% } else {%>
                                                                                            <span class="label label-primary">True</span>
                                                                                            <% } %>
                                                                                </li>

                                                                                <li class="list-group-item">
                                                                                    <strong>Promo:</strong>
                                                                                    <% if (!data.subscription.promo) { %>
                                                                                        <span class="label label-danger">False</span>
                                                                                        <% } else {%>
                                                                                            <span class="label label-primary">True</span>
                                                                                            <% } %>
                                                                                </li>
                                                                                <% if (data.subscription.promo) { %>
                                                                                    <li class="list-group-item">
                                                                                        <strong>Promo price:</strong>
                                                                                        <%= data.subscription.pricePromo %> $
                                                                                    </li>
                                                                                    <% } %>

                                                                                        <% if (data.subscription.promo) { %>
                                                                                            <li class="list-group-item">
                                                                                                <strong>Starts:</strong>
                                                                                                <%= data.subscription.sstarts %>
                                                                                            </li>
                                                                                            <% } %>
                                                                                                <% if (data.subscription.promo) { %>
                                                                                                    <li class="list-group-item">
                                                                                                        <strong>Ends:</strong>
                                                                                                        <%= data.subscription.sends %>
                                                                                                    </li>
                                                                                                    <% } %>


                                                                            </ul>

                                                                            <div class="row">
                                                                                <div class="col-md-4 col-sm-12"><a href="/admin/subscriptions?action=editSubscription&subscriptionId=<%= data.subscription._id %>" class="btn btn-block btn-primary">Edit</a></div>
                                                                                <div class="col-md-4 col-sm-12"><a data-id="<%= data.subscription._id %>" id="deleteSubscription" href="" class="btn btn-block btn-danger">Delete</a></div>
                                                                                <div class="col-md-4 col-sm-12"><a href="/admin/subscriptions" class="btn btn-block btn-outline btn-danger">Cancel</a></div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <% } %>

                                                                    <% if (data && data.action && data.action == 'editSubscription' && data.subscriptionId) { %>
                                                                        <div id="editUser" class="tab-pane active">
                                                                            <form role="form" id="editForm">
                                                                                <div class="row m-b-lg">

                                                                                    <div class="col-sm-12 text-center">
                                                                                        <h2>Edit subscription</h2>
                                                                                    </div>

                                                                                </div>
                                                                                <div class="client-detail">
                                                                                    <div class="">

                                                                                        <div id="error">

                                                                                        </div>

                                                                                        <div class="form-group  row">
                                                                                            <div class="col-sm-12"><label class="col-sm-12 row col-form-label">Name</label><input name="name" id="name" type="text" class="form-control" placeholder="Username" required></div>
                                                                                        </div>

                                                                                        <div class="form-group  row">
                                                                                            <div class="col-sm-12">
                                                                                                <label class="col-sm-12 row col-form-label">Role</label>
                                                                                                <select id="role" class="form-control" name="role" required>
                                                                                    <option value="" selected disabled>Select role</option>
                                                                                    
                                                                                    <option value="subscriber">Subscriber</option>
                                                                                    <option value="coach">Coach</option>
                                                                                    <option value="company">Company</option>
                                                                                </select>
                                                                                            </div>
                                                                                        </div>

                                                                                        <div class="form-group  row">
                                                                                            <div class="col-sm-12"><label class="col-sm-12 row col-form-label">Price</label><input type="number" id="price" name="price" max="9999" class="form-control" min="1" placeholder="Age"></div>
                                                                                        </div>

                                                                                        <div class="form-group  row">
                                                                                            <div class="col-sm-12">
                                                                                                <label class="col-sm-12 row col-form-label">Recursion</label>
                                                                                                <select id="recursion" class="form-control" name="recursion" required>
                                                                                    <option value="" selected disabled>Select recursion</option>
                                                                                    <option value="ot">One time</option>
                                                                                    <option value="weekly">Weekly</option>
                                                                                    <option value="monthly">Monthly</option>
                                                                                    <option value="yearly">Yearly</option>
                                                                                </select>
                                                                                            </div>
                                                                                        </div>

                                                                                        <div class="form-group  row">
                                                                                            <div class="col-sm-12">
                                                                                                <div class="i-checks"><label> <input id="public" type="checkbox"> <i></i> Publish ? </label></div>
                                                                                            </div>
                                                                                        </div>

                                                                                        <div class="form-group  row">
                                                                                            <div class="col-sm-12"><label class="col-sm-12 row col-form-label">Description</label><textarea class="form-control" name="description" id="description" placeholder="Description"></textarea>
                                                                                            </div>
                                                                                        </div>

                                                                                        <div class="form-group  row">
                                                                                            <div class="col-sm-12">
                                                                                                <div class="i-checks"><label> <input id="promo" type="checkbox"> <i></i> Promo ? </label></div>
                                                                                            </div>
                                                                                        </div>

                                                                                        <div class="form-group  row">
                                                                                            <div class="col-sm-12"><label class="col-sm-12 row col-form-label">Promo price</label><input type="number" id="pricePromo" name="pricePromo" max="9999" class="form-control" min="1" placeholder="Promo price"
                                                                                                    readonly></div>
                                                                                        </div>

                                                                                        <div class="form-group">
                                                                                            <label class="font-normal">Starts</label>
                                                                                            <div class="input-group date">
                                                                                                <span class="input-group-addon"><i class="fa fa-calendar"></i></span><input id="starts" name="starts" data-mask="99/99/9999" type="text" class="form-control"
                                                                                                    readonly>
                                                                                            </div>
                                                                                        </div>

                                                                                        <div class="form-group">
                                                                                            <label class="font-normal">Ends</label>
                                                                                            <div class="input-group date">
                                                                                                <span class="input-group-addon"><i class="fa fa-calendar"></i></span><input id="ends" name="ends" data-mask="99/99/9999" type="text" class="form-control" readonly>
                                                                                            </div>
                                                                                        </div>

                                                                                        <div class="form-group  row">
                                                                                            <div class="col-md-6 col-sm-12"><button type="submit" id="saveSubscription" class="btn btn-block btn-primary">Save</button></div>
                                                                                            <div class="col-md-6 col-sm-12"><a href="/admin/subscriptions" id="cancelSubscription" class="btn btn-block btn-outline btn-danger">Cancel</a>
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                            </form>
                                                                        </div>
                                                                        <% } %>
                                                    </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <%- include('../footer'); %>

                        </div>
            </div>



            <!-- Mainly scripts -->
            <script src="/assets/js/jquery-3.1.1.min.js"></script>
            <script src="/assets/js/popper.min.js"></script>
            <script src="/assets/js/bootstrap.js"></script>
            <script src="/assets/js/plugins/metisMenu/jquery.metisMenu.js"></script>
            <script src="/assets/js/plugins/slimscroll/jquery.slimscroll.min.js"></script>


            <script src="/assets/js/plugins/dataTables/datatables.min.js"></script>
            <script src="/assets/js/plugins/dataTables/dataTables.bootstrap4.min.js"></script>
            <script src="/assets/js/plugins/iCheck/icheck.min.js"></script>

            <!-- Jasny -->
            <script src="/assets/js/plugins/jasny/jasny-bootstrap.min.js"></script>
            <script src="/assets/js/jquery.cookie.js"></script>

            <!-- Date range use moment.js same as full calendar plugin -->
            <script src="/assets/js/plugins/fullcalendar/moment.min.js"></script>

            <!-- Switchery -->
            <script src="/assets/js/plugins/switchery/switchery.js"></script>

            <script src="/assets/js/plugins/jquery-ui/jquery-ui.min.js"></script>

            <!-- Jquery Validate -->
            <script src="/assets/js/plugins/validate/jquery.validate.min.js"></script>
            <script src="/assets/js/plugins/sweetalert/sweetalert.min.js"></script>
            <!-- Custom and plugin javascript -->
            <script src="/assets/js/inspinia.js"></script>
            <script src="/assets/js/plugins/pace/pace.min.js"></script>
            <script src="/assets/js/admin/subscriptions.js"></script>
            <script src="/assets/js/admin/logout.js"></script>
            <script src="/assets/js/commun.js"></script>

</body>

</html>