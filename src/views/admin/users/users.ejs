                                                                <option value="M">Male</option>
                                                                <option value="F">Female</option>
                                                            </select>
                                                                        </div>
                                                                    </div>

                                                                    <div class="form-group  row">
                                                                        <div class="col-md-6 col-sm-12"><label class="col-sm-12 row col-form-label">Name</label><input type="text" name="name" id="name" class="form-control" placeholder="Fullname" required></div>
                                                                        <div class="col-md-6 col-sm-12" id="ageContainer"><label class="col-sm-12 row col-form-label">Age</label><input type="number" id="age" max="999" class="form-control" min="1" placeholder="Age"></div>
                                                                    </div>

                                                                    <div class="form-group  row" id="aboutContainer">
                                                                        <div class="col-md-12"><label class="col-sm-12 row col-form-label">About</label><textarea class="form-control" name="about" id="about" placeholder="About"></textarea>
                                                                        </div>
                                                                    </div>

                                                                    <div class="form-group  row" id="AddressContainer">
                                                                        <div class="col-md-12"><label class="col-sm-12 row col-form-label">Billing address</label><textarea class="form-control" name="address" id="address" placeholder="Billing address" required></textarea>
                                                                        </div>
                                                                    </div>


                                                                    <div class="form-group  row" id="companyInfoContainer">
                                                                        <div class="col-md-6 col-sm-12"><label class="col-sm-12 row col-form-label">Number of employees</label><input type="number" name="nbrEmp" max="999999" min="1" id="nbrEmp" class="form-control" placeholder="Number of employees"
                                                                                required></div>
                                                                        <div class="col-md-6 col-sm-12"><label class="col-sm-12 row col-form-label">Tax number</label><input type="text" id="tax" name="tax" class="form-control" placeholder="Tax" required></div>
                                                                    </div>

                                                                    <div class="form-group  row" id="stateContainer">
                                                                        <div class="col-md-6 col-sm-12"><label class="col-sm-12 row col-form-label">State</label><input type="text" name="state" id="state" class="form-control" placeholder="State"></div>
                                                                        <div class="col-md-6 col-sm-12"><label class="col-sm-12 row col-form-label">City</label><input type="text" name="city" id="city" class="form-control" placeholder="City"></div>
                                                                    </div>

                                                                    <div class="form-group  row" id="countryContainer">
                                                                        <div class="col-sm-12">
                                                                            <label class="col-sm-12 row col-form-label">Country</label>
                                                                            <select id="country" class="form-control" name="country">
                                                                                <option value="" selected disabled>Select country
                                                                                </option>
                                                                                <% if (data && data.countries && data.countries.length) { %>
                                                                                    <% for(var i=0; i < data.countries.length; i++) {%>
                                                                                <option value="<%= data.countries[i] %>"><%= data.countries[i] %></option>
                                                                                <% } %>
                                                                                <% } %>
                                                                            </select>
                                                                        </div>
                                                                    </div>

                                                                    <div class="form-group  row">
                                                                        <div class="col-md-6 col-sm-12" id="zipcodeContainer"><label class="col-sm-12 row col-form-label">Zip Code</label><input type="text" name="zipcode" id="zipcode" class="form-control" placeholder="Zip Code" required></div>
                                                                        <div class="col-md-6 col-sm-12"><label class="col-sm-12 row col-form-label">Phone</label><input type="text" name="phone" id="phone" data-mask="(999) 999-9999" class="form-control" placeholder="Phone"></div>
                                                                    </div>

                                                                    <div class="form-group  row" id="weightContainer">
                                                                        <div class="col-md-12 col-sm-12"><label class="col-sm-12 row col-form-label">Weight</label><input type="number" name="weight" id="weight" min="1" max="300" class="form-control" placeholder="Weight"></div>
                                                                    </div>

                                                                    <div class="form-group  row" id="levelContainer">
                                                                        <label class="col-sm-12 col-form-label">Ellipsis Level: Speed / Time</label>
                                                                        <div class="col-md-6" id="speedContainer"><input type="number" name="speed" id="speed" min="1" max="20" class="form-control" placeholder="Speed"></div>
                                                                        <div class="col-md-6" id="timeContainer"><input type="number" name="time" id="time" min="1" max="10" class="form-control" placeholder="Time"></div>
                                                                    </div>

                                                                    <div class="form-group  row">
                                                                        <div class="col-md-6 col-sm-12"><button type="submit" id="saveUser" class="btn btn-block btn-primary">Save</button></div>
                                                                        <div class="col-md-6 col-sm-12"><a href="/admin/users" id="cancelUser" class="btn btn-block btn-outline btn-danger">Cancel</a>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </form>
                                                    </div>
                                                    <% } %>

                                                        <% if (data && data.action && data.action == 'showUser' && data.userId && data.user) { %>
                                                            <div id="showUser" class="tab-pane active">
                                                                <div id="error">

                                                                </div>
                                                                <div class="row m-b-lg">
                                                                    <div class="col-lg-4 text-center">
                                                                        <h2>
                                                                            <%= data.user.profile.name %>
                                                                        </h2>

                                                                        <div class="m-b-sm">
                                                                            <img alt="image" class="rounded-circle" src="<%= data.user.profile.picture %>" style="width: 62px;height: 62px;">
                                                                        </div>
                                                                    </div>

                                                                    <% if (data.user.profile.about) { %>
                                                                        <div class="col-lg-8">
                                                                            <strong>
                                                    About
                                                </strong>

                                                                            <p>
                                                                                <%= data.user.profile.about %>
                                                                            </p>

                                                                        </div>
                                                                        <% } %>

                                                                </div>
