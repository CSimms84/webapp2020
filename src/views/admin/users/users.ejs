<!DOCTYPE html>
<html>

<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>Ellipsis | Users</title>

    <link href="/assets/css/bootstrap.min.css" rel="stylesheet">
    <link href="/assets/font-awesome/css/font-awesome.css" rel="stylesheet">

    <!-- Toastr style -->
    <link href="/assets/css/plugins/toastr/toastr.min.css" rel="stylesheet">
    <link href="/assets/css/plugins/dataTables/datatables.min.css" rel="stylesheet">

    <link href="/assets/css/plugins/select2/select2.min.css" rel="stylesheet">
    <link href="/assets/css/plugins/select2/select2-bootstrap4.min.css" rel="stylesheet">

    <link href="/assets/css/plugins/switchery/switchery.css" rel="stylesheet">

    <link href="/assets/css/plugins/iCheck/custom.css" rel="stylesheet">
    <link href="/assets/css/plugins/jasny/jasny-bootstrap.min.css" rel="stylesheet">

    <link href="/assets/css/animate.css" rel="stylesheet">
    <link href="/assets/css/style.css" rel="stylesheet">
    <link href="/assets/css/admin/users.css" rel="stylesheet">
    <!-- Sweet Alert -->
    <link href="/assets/css/plugins/sweetalert/sweetalert.css" rel="stylesheet">
    <!-- Text spinners style -->
    <link href="/assets/css/plugins/textSpinners/spinners.css" rel="stylesheet">



</head>

<body class="pace-done skin-1">

    <div id="wrapper">
        <%- include('../navbar',{page:'users'}); %>
            <div id="page-wrapper" class="gray-bg">
                <%- include('../headerbar'); %>
                    <%- include('../breadcrumb',{page:'Users', items:['Admin', 'Users','Users']}); %>

                        <div class="wrapper wrapper-content  animated fadeInRight">
                            <div class="row">
                                <div class="col-sm-8">
                                    <div class="ibox">
                                        <div class="ibox-content">
                                            <span class="text-muted small float-right"><a id="" href="?action=adduser"
                                        class="btn btn-primary btn-circle client-link btn-add-user"> <i
                                            class="fa fa-plus"></i></a></span>
                                            <h2>Users</h2>



                                            <div class="clients-list">
                                                <span class="float-left small text-muted">
                                                 
                                    </span> Filter:
                                                <div class="i-checks">
                                                    <label> <input class="btn-role" data-role="all" type="radio" id="all"
                                                name="role"> <i></i> All </label>
                                                    <label> <input class="btn-role" data-role="admin" type="radio" id="admin"
                                                name="role"> <i></i> Admins </label>
                                                    <label> <input class="btn-role" data-role="subscriber" type="radio" id="subscriber"
                                                name="role"> <i></i> Subscriber </label>
                                                    <label> <input class="btn-role" data-role="coach" type="radio" id="coach"
                                                name="role"> <i></i> Coach </label>
                                                    <label> <input class="btn-role" data-role="company" type="radio" id="company"
                                                name="role"> <i></i> Company </label>
                                                </div>
                                                <hr>
                                                </hr>
                                                <div class="full-height-scroll">
                                                    <div class="table-responsive">
                                                        <table class="table table-striped table-bordered table-hover dataTables-example">
                                                            <thead>
                                                                <tr>
                                                                    <th>Photo</th>
                                                                    <th>Name</th>
                                                                    <th>UserName</th>
                                                                    <th>Phone</th>
                                                                    <th>email</th>
                                                                    <th>Country / Zip Code</th>
                                                                    <th>Level</th>
                                                                    <th>Role</th>
                                                                    <th>Status</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <% if (data && data.users && data.users.length) { %>
                                                                    <% for(var i=0; i < data.users.length; i++) {%>
                                                                        <tr>
                                                                            <td class="client-avatar">
                                                                                <a href="?action=showUser&userId=<%= data.users[i]._id %>" class="client-link">
                                                                <img alt="image"
                                                                    src="<%= data.users[i].profile.picture %>">
                                                                    <a href="?action=showUser&userId=<%= data.users[i]._id %>" class="pictureProfile" >
                                                                        <%= data.users[i].profile.picture %>
                                                                    </a>
                                                                                </a>
                                                                            </td>
                                                                            <td>
                                                                                <a href="?action=showUser&userId=<%= data.users[i]._id %>" class="client-link">
                                                                                    <%= data.users[i].profile.name %>
                                                                                </a>
                                                                            </td>
                                                                            <td>
                                                                                <a href="?action=showUser&userId=<%= data.users[i]._id %>" class="client-link">
                                                                                    <%= data.users[i].username %>
                                                                                </a>
                                                                            </td>



                                                                            <td>
                                                                                <% if (data.users[i].profile.phone) { %>
                                                                                    <i class="fa fa-phone"> </i>
                                                                                    <%= data.users[i].profile.phone %>
                                                                                        <% } %>
                                                                            </td>




                                                                            <td>
                                                                                <% if (data.users[i].email) { %>
                                                                                    <i class="fa fa-envelope"> </i>
                                                                                    <%= data.users[i].email %>
                                                                                        <% } %>
                                                                            </td>

                                                                            <td class="client-status">

                                                                                <% if (data.users[i].profile.city) { %>
                                                                                    <%= data.users[i].profile.city %>
                                                                                        <% } %>

                                                                                            <% if (data.users[i].profile.state) { %>,
                                                                                                <%= data.users[i].profile.state %>
                                                                                                    <% } %>
                                                                                                        <% if (data.users[i].profile.country) { %>
                                                                                                            <% if (data.users[i].profile.zipcode) { %>,
                                                                                                                <%= data.users[i].profile.zipcode %>
                                                                                                                    <% } %>
                                                                                                                        <%= data.users[i].profile.country %>
                                                                                                                            <% } %>
                                                                            </td>

                                                                            <td class="client-status">
                                                                                <% if (data.users[i].profile.speed && data.users[i].role != 'company') { %>
                                                                                    <%= data.users[i].profile.speed %>
                                                                                        <% if (data.users[i].profile.time && data.users[i].role != 'company') { %>/
                                                                                            <%= data.users[i].profile.time %>
                                                                                                <% } %>
                                                                                                    <% } %>
                                                                            </td>

                                                                            <td class="client-status">
                                                                                <span class="label label-info">
                                                                <%= data.users[i].role %></span>
                                                                            </td>
                                                                            <td class="client-status">
                                                                                <% if (data.users[i].disabled) { %>
                                                                                    <span class="label label-danger">Disabled</span>
                                                                                    <% } else {%>
                                                                                        <span class="label label-primary">Active</span>
                                                                                        <% } %>
                                                                            </td>
                                                                        </tr>
                                                                        <% } %>
                                                                            <% } %>

                                                            </tbody>

                                                        </table>
                                                    </div>
                                                </div>

                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-4">
                                    <div class="ibox selected">

                                        <div class="ibox-content">
                                            <div class="tab-content">
                                                <% if (data && data.action && data.action == 'adduser') { %>
                                                    <div id="addUser" class="tab-pane active">
