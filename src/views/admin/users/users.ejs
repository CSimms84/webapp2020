                                                                                <strong>Email:</strong>
                                                                                <%= data.user.email %>
                                                                            </li>
                                                                            <li class="list-group-item">
                                                                                <strong>Username:</strong>
                                                                                <%= data.user.username %>
                                                                            </li>
                                                                            <li class="list-group-item">
                                                                                <strong>Role:</strong>
                                                                                <%= data.user.role %>
                                                                            </li>
                                                                            <% if (data.user.profile.gender && data.user.role != 'company') { %>
                                                                                <li class="list-group-item">
                                                                                    <strong>Gender:</strong>

                                                                                    <% if (data.user.profile.gender == 'M') { %>
                                                                                        Male
                                                                                        <% } %>

                                                                                            <% if (data.user.profile.gender == 'F') { %>
                                                                                                Female
                                                                                                <% } %>
                                                                                </li>
                                                                                <% } %>
                                                                                    <% if (data.user.profile.age && data.user.role != 'company') { %>
                                                                                        <li class="list-group-item">
                                                                                            <strong>Age:</strong>
                                                                                            <%= data.user.profile.age %>
                                                                                        </li>
                                                                                        <% } %>


                                                                                            <% if (data.user.profile.nbrEmp && data.user.role == 'company') { %>
                                                                                                <li class="list-group-item">
                                                                                                    <strong>Number of employees:</strong>
                                                                                                    <%= data.user.profile.nbrEmp %>
                                                                                                </li>
                                                                                                <% } %>

                                                                                                    <% if (data.user.profile.tax && data.user.role == 'company') { %>
                                                                                                        <li class="list-group-item">
                                                                                                            <strong>Tax number:</strong>
                                                                                                            <%= data.user.profile.tax %>
                                                                                                        </li>
                                                                                                        <% } %>

                                                                                                            <% if (data.user.profile.address && data.user.role == 'company') { %>
                                                                                                                <li class="list-group-item">
                                                                                                                    <strong>Billing address:</strong>
                                                                                                                    <%= data.user.profile.address %>
                                                                                                                </li>
                                                                                                                <% } %>


                                                                                                                    <% if (data.user.profile.state) { %>
                                                                                                                        <li class="list-group-item">
                                                                                                                            <strong>State:</strong>
                                                                                                                            <%= data.user.profile.state %>
                                                                                                                        </li>
                                                                                                                        <% } %>


                                                                                                                            <% if (data.user.profile.city) { %>
                                                                                                                                <li class="list-group-item">
                                                                                                                                    <strong>City:</strong>
                                                                                                                                    <%= data.user.profile.city %>
                                                                                                                                </li>
                                                                                                                                <% } %>
                                                                                                                                    <% if (data.user.profile.country) { %>
                                                                                                                                        <li class="list-group-item">
                                                                                                                                            <strong>Country:</strong>
                                                                                                                                            <%= data.user.profile.country %>
                                                                                                                                        </li>
                                                                                                                                        <% } %>
                                                                                                                                            <% if (data.user.profile.zipcode) { %>
                                                                                                                                                <li class="list-group-item">
                                                                                                                                                    <strong>Zip Code:</strong>
                                                                                                                                                    <%= data.user.profile.zipcode %>
                                                                                                                                                </li>
                                                                                                                                                <% } %>
                                                                                                                                                    <% if (data.user.profile.weight && data.user.role != 'company') { %>
                                                                                                                                                        <li class="list-group-item">
                                                                                                                                                            <strong>Weight:</strong>
                                                                                                                                                            <%= data.user.profile.weight %>
                                                                                                                                                        </li>
                                                                                                                                                        <% } %>
                                                                                                                                                            <% if (data.user.profile.speed && data.user.role != 'company') { %>
                                                                                                                                                                <li class="list-group-item">
                                                                                                                                                                    <strong>Level:</strong>
                                                                                                                                                                    <%= data.user.profile.speed %>
                                                                                                                                                                        <% if (data.user.profile.time && data.user.role != 'company') { %>/
                                                                                                                                                                            <%= data.user.profile.time %>
                                                                                                                                                                                <% } %>
                                                                                                                                                                </li>
                                                                                                                                                                <% } %>
                                                                        </ul>

                                                                        <div class="row">
                                                                            <div class="col-md-4 col-sm-12"><a href="/admin/users?action=editUser&userId=<%= data.user._id %>" class="btn btn-block btn-primary">Edit</a></div>
                                                                            <div class="col-md-4 col-sm-12"><a data-id="<%= data.user._id %>" id="deleteUser" href="" class="btn btn-block btn-danger">Delete</a></div>
                                                                            <div class="col-md-4 col-sm-12"><a href="/admin/users" class="btn btn-block btn-outline btn-danger">Cancel</a></div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <% } %>

                                                                <% if (data && data.action && data.action == 'editUser' && data.userId) { %>
